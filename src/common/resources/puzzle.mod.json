{
  "formatVersion": 3,
  "display-name": "${p_name}",
  "id": "${p_id}",
  "version": "${p_version}",
  "description": "${p_desc}.",
  "authors": [ "Mr_Zombii", "CrabKing", "replet" ],
  "meta": {
    "icon": "${p_id}:assets/cosmic-api/icons/icon.png"
  },
  "entrypoints": {
    "postInit": [
      "io.github.puzzle.cosmic.CosmicServerAPI"
    ],
    "init": [
      "io.github.puzzle.cosmic.CosmicServerAPI"
    ],
    "client-postInit": [
      "io.github.puzzle.cosmic.CosmicClientAPI"
    ],
    "client-init": [
      "io.github.puzzle.cosmic.CosmicClientAPI"
    ]
  },
  "depends": {
    "puzzle-loader-core": ">=1.0.9-alpha",
    "puzzle-loader-cosmic": ">=1.4.1-alpha"
  },
  "allowedSides": {
    "client": true,
    "server": true
  },
  "mixins": [
    "puzzle_cosmic_api.mixins.json"
  ],
  "interface-injectors": [
    "cosmic-api-injector.inject"
  ],
  "access-writers": [
    "cosmic-api.manipulator"
  ]
}